page{
  background: #fafafa;
}
.wrapper{
  padding-bottom: 130rpx;
  width: 100%;
}
.header{
  height: 266rpx;
  width: 100%;
  background-size:100% 100%;
  background-repeat:no-repeat;
  overflow: hidden;
}
.header-top{
  margin: 24rpx auto 16rpx;
  width: 718rpx;
  display: flex;
  justify-content: space-between;
}
.header-top-left{
  width: 346rpx;
  display: flex;
}
.header-top-left .tou{
  width: 130rpx;
  height: 130rpx;
  background: #ffffcc;
  border-radius:10rpx; 
}
.header-top-left .tou image{
  display: block;
  height: 116rpx;
  width: 116rpx;
  margin: 8rpx;
  border-radius:10rpx; 
  background: #000;
}
.header-top-left  .names{
  color: #fff;
  font-size: 26rpx;
  line-height: 130rpx;
  margin-left: 12rpx;
}
.header-top-right{
  color: #fff;
  font-size: 20rpx;
}
.header-top-right button{
  background-size:100% 100%;
  background-repeat:no-repeat;
  width: 113rpx;
  height: 115rpx;
  padding: normal;  
  background-color:transparent; 
}
.header-top-right button::after{
  border: none;
}
.header-top-right image{
  width: 113rpx;
  height: 115rpx;
}
.header-bot{
  margin: 24rpx auto 16rpx;
  width: 718rpx;
  display: flex;
  justify-content: space-between;
  color: #fff;
  font-size: 26rpx;
}
.header-bot-right image{
  width: 153rpx;
  height: 69rpx;
}
.center{
  width: 728rpx;
  height: 123rpx;
  padding-top: 12rpx;
  margin: 0 auto;
  border-bottom:1rpx solid #e4e4e4; 
  border-left:1rpx solid #e4e4e4; 
  border-right:1rpx solid #e4e4e4; 
  border-bottom-left-radius:12rpx; 
  border-bottom-right-radius:12rpx; 

  display: flex;
  justify-content: space-between;
  text-align: center;
}
.center .center-item{
  width: 33.3%;
  color: #808080;
}
.center .center-item>view{
  width: 100%;
  font-size: 30rpx;
}
.center image{
  width: 37rpx;
  height: 37rpx;
  vertical-align: middle;
  margin-bottom: 5rpx;
}
.center view{
  width: 33.3%;
}
.center-tit{
  margin: 14rpx 0 0;
}
.mianImg{
  width: 723rpx;
  height: 200rpx;
  display: block;
  margin: 16rpx auto;
}
.titlist-item{
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  margin-bottom: 7rpx;
}
.tit-item-left{
  display: flex;
  color: #666666;
  font-size: 26rpx;
}
.titlist-item image{
  width: 45rpx;
  height: 45rpx; 
  display: block;
  margin: 20rpx 10rpx 0 50rpx;
}
.titlist button{
  padding: 0;
}
.titlist button::after{
  border: none;
}
.fot{
  position: fixed;
  height: 130rpx;
  width: 100%;
  background-size:100% 100%;
  background-repeat:no-repeat;
  bottom: 0;  
  display: flex;
  justify-content: space-between;
}
.fot view:nth-of-type(1){
  width: 252rpx;
  height: 100%;
}
.fot view:nth-of-type(2){
  width: 248rpx;
  height: 100%;
}
.fot view:nth-of-type(3){
  width: 260rpx;
  height: 100%;
}
.phone{
  width: 100%;
  height: 100%;
}
.phone button{
  width: 90%;
  display: block;
  margin:50rpx auto 114rpx;
}
.phone__header image{
  width: 160rpx;
  height: 160rpx;
  display: block;
  margin: 97rpx auto 34rpx;
}
.phone__header .titles{
  color: #666;
  font-weight: bold;
  text-align: center;
}
.phone__center .Ctitle{
  text-align: center;
  font-size: 33rpx;
  color: #a9a9a9;
  position: absolute;
  width: 277rpx;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  z-index: 99;
  background: #fafafa;
}
.phone__top{
  width: 708rpx;
  height: 52rpx;
  position: relative;
  margin: 0 auto;
}
.phone__top::after{
  position: absolute;
  content: "";
  display: block;
  width: 100%;
  height: 1rpx;
  background: #a9a9a9;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
}
.phonenums{
  width: 718rpx;
  height: 75rpx;
  position: relative;
  margin: 45rpx auto 18rpx;
}
.phonenums input{
  width: 626rpx;
  height: 100%;
  border: 4rpx solid #cacaca;
  border-radius: 16rpx;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 92rpx;
  font-size: 26rpx;
}
.phonenums image{
  height: 44rpx;
  width: 44rpx;
  top: 17rpx;
  left: 25rpx;
  position: absolute;
}
.yanzhengma{
  width: 718rpx;
  height: 75rpx;
  position: relative;
  margin:  0 auto 62rpx;
  display: flex;
  justify-content: space-between;
}
.yanzhengma image{
  height: 44rpx;
  width: 44rpx;
  top: 17rpx;
  left: 25rpx;
  position: absolute;
}
.yan__01{
  position: relative;
  width: 463rpx;
  height: 75rpx;
}
.yan__01 input{
  width: 369rpx;
  height: 100%;
  border: 4rpx solid #cacaca;
  border-radius: 16rpx;
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 92rpx;
  font-size: 26rpx;
}
.yan__02{
  width: 232rpx;
  height: 75rpx;
  line-height: 75rpx;
  color: #fff;
  font-size: 34rpx;
  background: #90c6fc;
  border-radius:15rpx;
  text-align: center; 
}
.phone__bot button{
  background: #3399ff;
  color: #fff;
}